
        <h1>User Page</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>SN</th>
            <th>Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% list.forEach((item,index) => {%>
            <tr>
                <td><%= index+1 %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.address %></td>
                <td><%= item.createdAt %></td>
                <td><%= item.updatedAt %></td>
                <td>

                </td>
            </tr>
        <% })%>
    </tbody>
</table>