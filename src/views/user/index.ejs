
        <h1>User Page</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>SN</th>
            <th>Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>Created At</th>
            <th>Updated At</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% list.forEach((item,index) => {%>
            <tr>
                <td><%= index+1 %></td>
                <td><%= item.name %></td>
                <td><%= item.email %></td>
                <td><%= item.address %></td>
                <td><%= dayjs(item.createdAt).format("YYYY-MM-DD") %></td>
                <td><%= item.updatedAt %></td>
                <td>
                    <a href="/user/edit/<%= item.id %>" class="btn btn-primary btn-sm">Edit</a>
                    <a href="/user/delete/<%= item.id %>" class="btn btn-danger btn-sm">Delete</a>
                </td>
            </tr>
        <% })%>
    </tbody>
</table>